<h1>Tasks#show</h1>
<%= link_to 'index', tasks_path, class: 'btn btn-primary'%>
<table class="table.table-hover">
  <tbody>
    <tr>
      <th>title</th>
      <td><%= @task.title %></td>
    </tr>
    <tr>
      <th>content</th>
      <td><%= @task.content %></td>
    </tr>
    <tr>
      <th>deadline</th>
      <td><%= @task.deadline %></td>
    </tr>
    <tr>
      <th>status</th>
      <td><%= @task.status %></td>
    </tr>
    <tr>
      <th>priority</th>
      <td><%= @task.priority %></td>
    </tr>
 </tbody>
</table>
